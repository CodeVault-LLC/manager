{
  "name": "manager",
  "version": "1.0.0",
  "description": "Manage your applications with ease",
  "main": "./out/main/index.js",
  "type": "module",
  "author": {
    "name": "Lukas Olsen",
    "email": "lukmarwil@gmail.com"
  },
  "repository": {
    "url": "https://github.com/codevault-llc/manager",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/codevault-llc/manager/issues"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --cache --cache-strategy content && prettier --check .",
    "lint:fix": "eslint . --cache --cache-strategy content --fix",
    "lint:clear": "rimraf .eslintcache",
    "prettier": "prettier --cache --write .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "grpc:generate": "grpc_tools_node_protoc_ts --ts_out=src/grpc --grpc_out=src/grpc --proto_path=src/grpc src/grpc/*.proto",
    "database:generate": "drizzle-kit generate"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@eslint/js": "^9.27.0",
    "@faker-js/faker": "^9.8.0",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@libsql/client": "^0.15.8",
    "@mdx-js/mdx": "^3.1.0",
    "@radix-ui/react-popover": "^1.1.14",
    "@tanstack/react-form": "^1.11.3",
    "@tanstack/router-plugin": "^1.120.7",
    "archiver": "^7.0.1",
    "axios": "^1.9.0",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.41.0",
    "electron-updater": "^6.6.2",
    "input-otp": "^1.4.2",
    "keen-slider": "^6.8.6",
    "lucide-react": "^0.476.0",
    "next-themes": "^0.4.6",
    "semver": "^7.7.2",
    "sonner": "^2.0.3",
    "swiper": "^11.2.7",
    "systeminformation": "^5.26.2",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-proto": "^2.7.0",
    "winston": "^3.17.0",
    "winston-format": "^1.0.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tanstack/react-router": "^1.120.7",
    "@tanstack/router-devtools": "^1.120.7",
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.7",
    "@types/semver": "^7.7.0",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.30.6",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.27.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
